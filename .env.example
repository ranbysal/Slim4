PORT=3000
LOG_LEVEL=info
DB_PATH=./data/bot.sqlite
RPC_WS_URL_PRIMARY=wss://YOUR_PRIMARY_WS
RPC_HTTP_URL_PRIMARY=https://YOUR_PRIMARY_HTTP
RPC_WS_URL_BACKUP=wss://YOUR_BACKUP_WS
RPC_HTTP_URL_BACKUP=https://YOUR_BACKUP_HTTP
JITO_BLOCK_ENGINE_URL=https://mainnet.block-engine.jito.wtf
JITO_TIP_BUDGET_DAILY_SOL=0.2
JITO_TIP_MAX_PER_TRADE_SOL=0.05
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Feeds
# Comma-separated list of program IDs for each origin
PUMPFUN_PROGRAM_IDS=
LETSBONK_PROGRAM_IDS=
MOONSHOT_PROGRAM_IDS=
RAYDIUM_PROGRAM_IDS=
ORCA_PROGRAM_IDS=
FEEDS_ENABLED=true

# Entry / Dry run
DRY_RUN=true
SIZE_SMALL_USD=30
SIZE_APEX_USD=120
ENTRY_MIN_SCORE=60
ENTRY_APEX_SCORE=80
DECISION_COOLDOWN_SEC=120

# Adaptive entry thresholds and cooldowns
MIN_OBS_BUYERS=8
MIN_OBS_UNIQUE=6
REEVAL_COOLDOWN_SEC=15
ACCEPT_COOLDOWN_SEC=120

# Alerts / Telegram
ALERTS_ACCEPTED_ONLY=false
ALERTS_MIN_SCORE=60
ALERTS_RATE_LIMIT_SEC=1
# 0 disables; e.g. 300 for 5m summaries
ALERTS_SUMMARY_EVERY_SEC=300

# Market Heat auto-tuner
HEAT_ENABLED=true
HEAT_WINDOW_MIN=10
HEAT_MIN_ACCEPTS_PER_HR=2
HEAT_MAX_ACCEPTS_PER_HR=12
HEAT_LOOSEN_DELTA_SCORE=10
HEAT_LOOSEN_DELTA_BUYERS=2
HEAT_TIGHTEN_DELTA_SCORE=10
HEAT_TIGHTEN_DELTA_BUYERS=2
HEAT_FLOOR_SCORE=40
HEAT_FLOOR_BUYERS=2
HEAT_CEIL_SCORE=95
HEAT_CEIL_BUYERS=16
# Note: When tape is COLD, Slim4 enforces higher effective floors
# without changing apex floor. Effective floors under COLD:
#   - minScore: at least 50
#   - minBuyers: at least 6
#   - minUnique: at least 4
# The clamp to HEAT_*_CEIL values still applies.

# Pending TTL controls
HOLD_TTL_SEC=300
HOLD_MAX_REEVALS=20

# Smart-money cohort
# Comma-separated list of wallet pubkeys (base58) to treat as cohort
COHORT_WALLETS=
# Boost added to conviction score if a cohort wallet bought recently
COHORT_BOOST=15
# Boost decay window in seconds
COHORT_DECAY_SEC=600

# Deployer graduation-rate CSV (path)
DEPLOYER_GR_CSV=./data/deployer_gr.csv
